<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.addsensor.CameraMap" android:versionCode="3"
	android:versionName="1.5.0">
	<application android:icon="@drawable/cameramap_icon" android:label="@string/app_name" android:debuggable="true">
		<uses-library android:name="com.google.android.maps" />
		<activity android:name=".Camera" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".CameraMap" android:label="@string/map">
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name=".Form" android:label="@string/form">
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.DEFAULT" />
				<type android:value=".CameraMap" />
			</intent-filter>
		</activity>
		<activity android:name=".Login" android:label="@string/login">
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.DEFAULT" />
				<type android:value=".CameraMap" />
			</intent-filter>
		</activity>
		<activity android:name=".CreateAccount" android:label="@string/createAccount">
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.DEFAULT" />
				<type android:value=".CameraMap" />
			</intent-filter>
		</activity>
		<activity android:name=".CameraAR" android:label="@string/cameraAR">
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.DEFAULT" />
				<type android:value=".CameraMap" />
			</intent-filter>
		</activity>
</application>
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.CAMERA" />
 	<uses-feature android:name="android.hardware.CAMERA" />
 	<uses-feature android:name="android.hardware.CAMERA.AUTOFOCUS" />
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
	<uses-permission android:name="android.permission.ACCESS_MOCK_LOCATION" />
	<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
	<uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />

</manifest> 